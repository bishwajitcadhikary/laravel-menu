{
   "name": "bishwajitcadhikary/laravel-menu",
   "description": "Menu and sidebar management package for Laravel",
   "keywords": [
       "laravel",
       "menu",
       "navigation",
       "sidebar",
       "bootstrap"
   ],
   "license": "MIT",
   "authors": [
        {
            "name": "Bishwajit Adhikary",
            "email": "bishwajitcadhikary@gmail.com",
            "homepage": "https://wovosoft.com/bishwajit",
            "role": "Developer"
        }
   ],
   "require": {
        "php": ">=7.3",
        "illuminate/config": ">=5.5",
        "illuminate/support": ">=5.5",
        "illuminate/view": ">=5.5",
        "laravelcollective/html": ">=5.5"
   },
   "require-dev": {
        "friendsofphp/php-cs-fixer": ">=3.4",
        "mockery/mockery": ">=1.4",
        "orchestra/testbench": ">=6.0",
        "phpunit/phpunit": ">=9.0"
   },
   "autoload": {
        "psr-4": {
            "WovoSoft\\Menu\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
   },
    "autoload-dev": {
        "psr-4": {
            "WovoSoft\\Menu\\Tests\\": "tests"
        }
    },
   "extra": {
        "laravel": {
            "providers": [
                "WovoSoft\\Menu\\Provider"
            ],
            "aliases": {
                "Menu": "WovoSoft\\Menu\\Facade"
            }
        }
   }
}
